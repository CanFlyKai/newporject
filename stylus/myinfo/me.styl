body
	background #efeff4
	.header
		background #fff
		text-align center
		height 50px
		.header-title
			color #000
			font-size 18px
			line-height 50px
		.set
			width 50px
			height 50px
			background url("../../img/myinfo/nav_set_icon@2x.png") center no-repeat
			background-size 18px
			border none
			outline none
			float right
	.info-box
		background #fff
		margin-top 10px
		.info-main
			overflow hidden
			height 100px
			width 100%
			position relative
			&:after
				content ""
				position absolute
				bottom 0
				left 0
				width 100%
				height 1px
				background #d6d7dc
			.info-img
				width 65px
				height 65px
				border-radius 50%
				background url("../../img/myinfo/log in_user-head@2x.png") center no-repeat
				background-size cover
				float left
				margin-left 5%
				margin-top 17px
			.info-text
				line-height 100px
				float left
				padding-left 15px
			.info-next
				width 30px
				height 100px
				background url("../../img/myinfo/log in_right_icon@2x.png") center no-repeat
				float right
				background-size 12px
		.info-foot
			overflow hidden
			.info-foot-cell
				float left
				line-height 45px
				height 45px
				width 50%
				text-align center
				.info-numb 
					font-weight bold
					font-size 18px
			.rightline
				position relative
				&:after
					content ""
					position absolute
					width 1px
					height 33px
					right 0px
					top 6px
					background #d6d7dc
	.navbar
		background #fff
		margin-top 	10px
		height 50px
		line-height 50px
		over-flow hidden
		.bar-right
			float right
			width 40px
			height 50px
			background url("../../img/myinfo/log in_right_icon@2x.png") center no-repeat
			background-size 8px
		.bar-left
			height 50px
			float left
			padding-left 40px
			font-size 16px
		.fst
			background url("../../img/myinfo/content_list_icon01@2x.png") 10px center no-repeat
			background-size 18px
		.scd
			background url("../../img/myinfo/content_list_icon02@2x.png") 10px center no-repeat
			background-size 18px		
		.thd
			background url("../../img/myinfo/content_list_icon03@2x.png") 10px center no-repeat
			background-size 18px
		.for
			background url("../../img/myinfo/content_list_icon04@2x.png") 10px center no-repeat
			background-size 18px
		.fif
			background url("../../img/myinfo/content_list_icon05@2x.png") 10px center no-repeat
			background-size 18px	
	.footbox
		position absolute
		bottom 0
		background #fff
		display flex
		width 100%
		.foot-cell 
			flex 1
			height 20px
			padding-top 30px
			text-align center
			color #b4b4b4
			font-size 12px
		.cellone
			background  url("../../img/myinfo/bottom_nor_icon04@2x.png") center 5px no-repeat
			background-size 24px
		.celltwo
			background  url("../../img/myinfo/bottom_nor_icon03@2x.png") center 5px no-repeat
			background-size 24px	
		.cellthr
			background  url("../../img/myinfo/bottom_icon05@2x.png") center no-repeat
			background-size 45px	
		.cellfor
			background  url("../../img/myinfo/bottom_nor_icon02@2x.png") center 5px no-repeat
			background-size 24px			
		.cellfif
			background  url("../../img/myinfo/bottom_nor_icon01@2x.png") center 5px no-repeat
			background-size 24px			
					
					
					
					
					
					
					